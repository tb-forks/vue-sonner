{
  "name": "@tb-dev/vue-sonner",
  "description": "Sonner for Vue",
  "type": "module",
  "version": "0.1.1",
  "packageManager": "pnpm@10.29.3",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tb-forks/vue-sonner.git"
  },
  "scripts": {
    "build": "vite build",
    "format": "dprint fmt",
    "release": "pnpm run build && pnpm publish",
    "type-check": "vue-tsc --noEmit"
  },
  "dependencies": {
    "vue": "^3.5.28"
  },
  "devDependencies": {
    "@types/node": "^25.2.3",
    "@vitejs/plugin-vue": "^6.0.4",
    "tslib": "^2.8.1",
    "typescript": "~5.9.3",
    "vite": "^7.3.1",
    "vite-plugin-dts": "=4.5.4",
    "vue-tsc": "^3.2.4"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./style.css": "./dist/index.css"
  },
  "files": [
    "dist"
  ]
}
